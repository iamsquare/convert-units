{
  "name": "@iamsquare/convert-units",
  "version": "0.1.1",
  "description": "Convert between quantities in different units",
  "author": "Marco Cesi <sq@iamsquare.it> (https://iamsquare.it)",
  "keywords": [
    "metric",
    "imperial",
    "convert",
    "ratio",
    "fraction"
  ],
  "bugs": {
    "url": "https://github.com/iamsquare/convert-units/issues"
  },
  "dependencies": {
    "ramda": "^0.27.1",
    "ramda-adjunct": "^2.33.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/ramda": "^0.27.40",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^6.0.0",
    "is-ci": "^3.0.0",
    "jest": "^27.0.1",
    "lint-staged": "^11.0.0",
    "prettier": "2.3.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.1",
    "typescript": "^4.3.2"
  },
  "files": [
    "lib/"
  ],
  "homepage": "https://iamsquare.js/convert-units",
  "license": "MIT",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/iamsquare/convert-units.git"
  },
  "scripts": {
    "build": "npm run clean:lib && tsc -p ./tsconfig-esm.json && tsc -p ./tsconfig-cjs.json && tsc -p ./tsconfig-umd.json && tsc -p ./tsconfig-d.json",
    "clean": "rimraf",
    "clean:lib": "npm run clean -- lib",
    "lint": "eslint . --quiet",
    "lint:fix": "npm run lint -- --fix",
    "lint-staged": "lint-staged",
    "prettier": "prettier --write",
    "test": "jest",
    "test:coverage": "npm run test -- --coverage",
    "test:watch": "npm run test -- --watch",
    "postinstall": "is-ci || husky install"
  },
  "types": "lib/types/index.d.ts"
}
